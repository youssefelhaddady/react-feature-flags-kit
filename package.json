{
  "name": "react-feature-flags-package",
  "version": "0.0.1",
  "description": "Manage react features flags easily and effectively",

  "type": "module",
  "license": "MIT",
  "private": true,
 
 "author": {
   "name": "Youssef EL HADDADY",
   "email": "YoussefELHADDADY@gmail.com"
 },
 "repository": {
   "type": "git",
   "url": "https://github.com/youssefelhaddady/react-feature-flags-kit"
 },

  "workspaces": [
    "lib/*",
    "demo/*"
  ],
  "scripts": {
